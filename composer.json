{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "blade-ui-kit/blade-heroicons": "^0.2.2",
        "blade-ui-kit/blade-icons": "^0.4.3",
        "facade/ignition": "^2.3.6",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/nova": "^3.9",
        "laravel/tinker": "^2.0",
        "spatie/calendar-links": "^1.4",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-flash": "^1.7",
        "spatie/laravel-honeypot": "^3.0",
        "spatie/laravel-mailcoach": "^2.15",
        "spatie/laravel-mailcoach-monaco": "^1.1",
        "spatie/laravel-tail": "^4.2",
        "wildbit/swiftmailer-postmark": "^3.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "fzaninotto/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-log-dumper": "^1.2",
        "spatie/laravel-stubs": "^1.1"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish",
            "@php artisan vendor:publish --tag mailcoach-assets --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ]
}
